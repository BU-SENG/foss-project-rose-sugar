# Backend API Configuration
# Copy this file to .env.local and fill in your backend URL

# Django Backend Setup
VITE_DJANGO_BACKEND=true

# API Base URL - Update this to your Django backend server
# For Django: typically http://localhost:8000/api
VITE_API_URL=http://localhost:8000/api

# Optional: Add auth token if using JWT
# VITE_AUTH_TOKEN=your_jwt_token_here

# Optional: Enable debugging
VITE_DEBUG=false
